  "bugs": {
    "url": "https://github.com/Dhiraj-virani/VKart/issues"
  },
  "homepage": "https://github.com/Dhiraj-virani/VKart#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^5.10.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
